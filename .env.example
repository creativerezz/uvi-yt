# YouTube API Server Environment Variables
# Copy this file to .env and fill in your values

# Server Configuration
HOST=0.0.0.0
PORT=8000
LOG_LEVEL=INFO

# Application Settings
PROJECT_NAME="YouTube API Server"
DEBUG=false

# CORS Settings
# For development: use "*" to allow all origins
# For production: use comma-separated list of allowed domains
# Examples:
#   Development: BACKEND_CORS_ORIGINS=*
#   Production: BACKEND_CORS_ORIGINS=https://yourdomain.com,https://app.yourdomain.com
BACKEND_CORS_ORIGINS=*

# Optional: YouTube Data API Key (for enhanced metadata)
# YOUTUBE_API_KEY=your-youtube-api-key-here

# Proxy Configuration
# Set PROXY_TYPE to "webshare" to use Webshare rotating proxies
PROXY_TYPE=webshare
WEBSHARE_USERNAME=your-username-here
WEBSHARE_PASSWORD=your-password-here
# WEBSHARE_API_TOKEN=your-api-token-here
